{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint:fix": "eslint --fix .",
    "tsc": "tsc --noEmit",
    "prettier": "prettier . --write",
    "prettier:check": "prettier --check",
    "format": "npm run prettier && npm run lint:fix",
    "migration:deploy": "dotenv -e ../.local.env -- prisma migrate deploy",
    "seed": "dotenv -e ../.local.env -- ts-node ./prisma/seed.ts",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./node_modules/.bin/jest --runInBand",
    "start:dev": "nest start --config tsconfig.json --watch"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.58",
    "eslint-plugin-prettier": "^4.0.0",
    "global": "^4.4.0",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prettier": "^2.8.8",
    "prisma": "^5.18.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.1",
    "@types/jest": "^29.5.12",
    "run-script-webpack-plugin": "^0.2.0",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "wrap-ansi": "7.0.0"
  }
}
